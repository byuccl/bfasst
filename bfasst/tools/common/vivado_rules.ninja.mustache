rule vivado
    command = cd $cwd && $$BFASST_UTILS/retry.sh $$VIVADO_BIN -mode batch -journal $journal -log $log -source $in >&- 
    description = vivado $in

{{#in_context}}
rule vivado_ioparse
    command = python {{ bfasst_path }}/bfasst/utils/vivado_ioparse.py <$in >$out
    description = vivado_ioparse <$in >$out
    
{{/in_context}}
