rule bit_to_fasm
    command = PATH=$PATH:$xray_path $fasm2bels_python_path $bit_to_fasm_path --db-root $db_root --part $part $in >$out
    description = convert $in to fasm file

rule fasm_to_netlist
    command = $fasm2bels_python_path -mfasm2bels --connection_database $part_db --db_root $db_root --part $part --fasm_file $in --verilog_file $verilog_file --xdc_file $xdc_file --input_xdc $input_xdc
    description = convert $fasm_file to netlist
